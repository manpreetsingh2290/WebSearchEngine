<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="WebApp_ID" version="3.1">
  <display-name>HttpSearch</display-name>
  <welcome-file-list>
    <welcome-file>mainpage.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>AutoCompleteController</servlet-name>
    <servlet-class>com.servlet.AutoCompleteController</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>SerachWordController</servlet-name>
    <servlet-class>com.servlet.SerachWordController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AutoCompleteController</servlet-name>
    <url-pattern>/AutoCompleteController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SerachWordController</servlet-name>
    <url-pattern>/SerachWordController</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>StaticPageController</servlet-name>
    <servlet-class>com.servlet.StaticPageController</servlet-class>
  </servlet>
  
   <servlet-mapping>
    <servlet-name>StaticPageController</servlet-name>
    <url-pattern>/StaticPageController/*</url-pattern>
  </servlet-mapping>
  
</web-app>